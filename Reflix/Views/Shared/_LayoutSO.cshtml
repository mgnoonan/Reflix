<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
    </head>
<body class="home-page">
    <!--noscript><div id="noscript-padding"></div></noscript>
    <div id="notify-container"></div>
    <div id="overlay-header"></div-->
    <div id="custom-header"></div>
    <div class="container">
        <div id="header">
            <div id="portalLink">
                <!--a class="genu" href="http://stackexchange.com" onclick="StackExchange.ready(function(){genuwine.click();});return false;">Stack Exchange</a-->
            </div>
            <div id="topbar">
                <div id="hlinks">
                    <span id="hlinks-user">
                  <a href="#" class="profile-link">@ViewBag.Username</a>&nbsp;<!--a href="/users/88672/mgnoonan?tab=reputation"><span class="reputation-score" title="your reputation; view reputation changes">133</span></a><span title="1 silver badge"><span class="badge2"></span><span class="badgecount">1</span></span><span title="6 bronze badges"><span class="badge3"></span><span class="badgecount">6</span></span-->
 <span class="lsep">|</span>
                  </span>
                  <span id="hlinks-nav"></span>
                  <span id="hlinks-custom">
                    @Html.ActionLink("about", "About", "Home")
                    <span class="lsep">|</span>
                    <a href="/faq">faq</a>
                  </span>
                </div>
                <div id="hsearch">
                    <form id="search" action="/search" method="get">
                    <div>
                        <input name="q" class="textbox" tabindex="1" onfocus="if (this.value=='search') this.value = ''" type="text" maxlength="140" size="28" value="search">
                    </div>
                    </form>
                </div>
            </div>
            <br class="cbt">
            <div id="hlogo">
                <a href="/">Reflix</a>
            </div>
            <div id="hmenus">
                <div class="nav mainnavs">
                    @{
                        string controller = ViewContext.RouteData.Values["Controller"].ToString().ToLowerInvariant();
                        string action = ViewContext.RouteData.Values["Action"].ToString().ToLowerInvariant();
                    }
                    <ul>
                        <li class="@(controller == "enduser" ? "youarehere" : "")">@Html.ActionLink("End User", "Index", "EndUser", null, new { id = "nav-home" })</li>
                    </ul>
                </div>
                <div class="nav askquestion">
                    <ul>
                        <li class="@(controller == "ticket" && action == "create" ? "youarehere" : "")">
                            @Html.ActionLink("New Ticket", "Create", "Ticket", null, new { id = "nav-askquestion" })
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div id="content">
            
@RenderBody()

